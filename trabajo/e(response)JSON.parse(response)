erie riutilizzabili[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eOttimizzare i componenti creati per ottenere massime performances sull'intera gamma di device/browser supportati[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eUtilizzare (e quando necessario creare da zero) client e librerie per consumare servizi REST[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eUtilizzare strumenti di sviluppo e build (Babel, Webpack, NPM, [1;35m\xE2[0m[31m[1;35m\x80[0m[31m[1;35m\xA6[0m[31m)[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eRevisionare le pull requests dei tuoi colleghi[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eLavorare in pair programming[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003c/ul[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cp[1;35m\\[0m[31mu003eBonus points:[1;35m\\[0m[31mu003c/p[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cul[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eReactJS, Redux e isomorphic React[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eWebSocket[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003eLibrerie di data structuring (es. immutable.js)[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[31mu003cli[1;35m\\[0m[31mu003ePattern di autenticazione (OAuth2, JWT, [1;35m\xE2[0m[31m[1;35m\x80[0m[31m[1;35m\xA6[0m[31m)[1;35m\\[0m[31mu003c/li[1;35m\\[0m[31mu003e[1;35m\\[0m[31mn[1;35m\\[0m[